{
  "name": "reading-list-manager",
  "version": "1.0.0",
  "description": "A full-stack web application for managing personal reading lists",
  "main": "backend/server.js",
  "scripts": {
    "start": "cd backend && npm start",
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\"",
    "backend:dev": "cd backend && npm run dev",
    "frontend:dev": "cd frontend && npm run dev",
    "backend:start": "cd backend && npm start",
    "frontend:build": "cd frontend && npm run build",
    "build": "npm run frontend:build",
    "db:init": "node db/init.js",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules"
  },
  "keywords": [
    "reading-list",
    "books",
    "svelte",
    "express",
    "sqlite",
    "full-stack"
  ],
  "author": "Marcelo and Umar",
  "license": "whatever really",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marceloamor/reading-list-manager.git"
  },
  "bugs": {
    "url": "https://github.com/marceloamor/reading-list-manager/issues"
  },
  "homepage": "https://github.com/marceloamor/reading-list-manager#readme"
}
